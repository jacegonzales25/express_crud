{
  "name": "express-post",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev:nodemon": "nodemon .bin/www",
    "dev:tailwind": "npx tailwindcss -i ./static/style.css -o ./public/stylesheets/style.css --watch",
    "dev": "concurrently \"npm run dev:tailwind\" \"npm run dev:nodemon \" -n \"nodemon|tailwind\" --name-separator \"|\"", 
    "type": "module"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.1",
    "preline": "^1.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "daisyui": "^3.8.1",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.3"
  }
}
